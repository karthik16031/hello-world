get-view "Package Explorer" | get-tree | select "BRP-models/L3" | get-menu "New/New Enquiry..." | click
with [get-window "New Enquiry"] {
    with [get-group "Enquiry Details"] {
        get-editbox -after [get-label "Enquiry Name:"] | set-text "RCPTT.TEST.ENQ"
        get-button "Browse..." | click
    }
    with [get-window "Application Selection"] {
        get-editbox -after [get-label "Choose an Application:"] | set-text CUSTOMER
        get-table | select CUSTOMER
        get-button OK | click
        
        
    }
    get-button "Next >" | click
    with [get-group "Field Selection"] {
        with [get-group "Application Fields"] {
            get-editbox -after [get-label "Mandatory Fields"] | set-text LANGUAGE
            get-tree | select LANGUAGE
        }
        get-button ">" | click
        with [get-group "Application Fields"] {
            get-editbox -after [get-label "Mandatory Fields"] | set-text MNEMONIC
            get-tree | select MNEMONIC
        }
        get-button ">" | click
        with [get-group "Application Fields"] {
            get-editbox -after [get-label "Mandatory Fields"] | set-text "NAME.1"
            get-tree | select "NAME.1"
        }
        get-button ">" | click
        with [get-group "Application Fields"] {
            get-editbox -after [get-label "Mandatory Fields"] | set-text SECTOR
            get-tree | select SECTOR
        }
        get-button ">" | click
        with [get-group "Application Fields"] {
            get-editbox -after [get-label "Mandatory Fields"] | set-text "SHORT.NAME"
            get-tree | select "SHORT.NAME"
        }
        get-button ">" | click
    }
    get-button Finish | click
    
    
}
