Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "BRP-models/L3" | get-menu "New/New Menu..." | click
with [get-window "New Menu"] {
    get-editbox -after [get-label "Menu Name :"] | set-text "RCPTT.TEST.MENU"
    get-button Finish | click
}
with [get-editor "RCPTT.TEST.MENU.menu" | get-tree] {
    get-item "RCPTT.TEST.MENU" | click
    get-item "RCPTT.TEST.MENU" | click
    select "RCPTT.TEST.MENU" | get-menu "New Item" | click
    get-item "RCPTT.TEST.MENU/menu" | click
}
with [get-view Properties] {
    click
    get-group "" | get-editbox -after [get-label "Name :"] | set-text SubMenu1
    with [get-group "Resource:"] {
        get-button Enquiry | click
        get-group Enquiry | get-button Browse | click
    }
}
with [get-window "Open Model Element"] {
    get-editbox -after [get-label "Enter element name prefix or pattern (*, ?, or camel case):"] 
        | set-text "*RCPTT.TEST"
    get-table | select "RCPTT.TEST.ENQ - Enquiry"
    get-button OK | click
}
with [get-editor "RCPTT.TEST.MENU.menu" | get-tree] {
    get-item "RCPTT.TEST.MENU" | click
    get-item "RCPTT.TEST.MENU" | click
    select "RCPTT.TEST.MENU" | get-menu "New Item" | click
}
with [get-view Properties] {
    get-group "" | get-editbox -after [get-label "Name :"] | set-text SubMenu2
    with [get-group "Resource:"] {
        get-button Version | click
        get-group Version | get-button Browse | click
    }
}
with [get-window "Open Model Element"] {
    get-editbox -after [get-label "Enter element name prefix or pattern (*, ?, or camel case):"] | set-text "*RCPTT"
    get-table | select "CUSTOMER,RCPTT.TEST.VI - Version"
    get-button OK | click
}
with [get-view Properties | get-group "Resource:" | get-group Version | get-button Browse] {
    key-type "M1+s"
    }
with [get-editor "RCPTT.TEST.MENU.menu" | get-tree] {
    get-item "RCPTT.TEST.MENU" | click
    key-type "M1+g"
}
get-editor "RCPTT.TEST.MENU.menu" | get-tree | key-type "M1+s"
get-view Console | get-text-viewer | get-property "getText()" | contains "HELPTEXT.MENU/HELPTEXT.MENU!RCPTT.TEST.MENU.data.eson SUCCEEDED" | verify-true


get-view "Package Explorer" | click
get-editor "RCPTT.TEST.MENU.menu" | close
