get-menu "File/Open Projects from File System..." | click
set-dialog-result Folder $Workspace_Home
with [get-window "Import Projects from File System or Archive"] {
    get-button "Directory..." | click
    get-button Finish | click
}
get-view Servers | get-tree | select "T24 Connection ( t24-localhost-tafj-server )" 
    | get-menu "Add\\/Remove Project(s)" | click
with [get-window "Add/Remove Projects"] {
    with [get-tree] {
        select "BRP-data-code"
        get-item "BRP-data-code" | check
        get-item "BRP-models-gen" | check
    }
    get-button OK | click
     
}
get-view Servers | get-tree | select "T24 Connection ( t24-localhost-tafj-server )" | get-menu "Start \\/ Restart" 
    | click
wait -ms 300000
get-view Console | get-text-viewer | get-property "getText()" 
    | contains "started" | verify-true
