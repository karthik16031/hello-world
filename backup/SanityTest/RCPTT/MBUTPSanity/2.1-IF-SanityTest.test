Entry-Name: .content

get-button "Open Perspective" | click
get-window "Open Perspective" | get-table | select "Integration Studio" | double-click
get-menu "File/New/T24 Integration Project" | click
with [get-window "New Integration Project"] {
    get-editbox -after [get-label "Project Name:"] | set-text IFTEST
    with [get-group "Connection Properties"] {
        get-button "Enable project specific settings" | check
        get-group "Connection Method" | get-button "Web Service" | click
        with [get-group "Web Connection Details" | get-editbox -after [get-label "IF Services URL :"]] {
            click-text 22
            key-type "M2+ARROW_LEFT" -times 4
            set-text "http://localhost:9089/axis2/"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-group "User Credentials"] {
            with [get-editbox -after [get-label "User Name:"]] {
                set-text INPUTT
                key-type "TRAVERSE_TAB_NEXT"
            }
            get-editbox -after [get-label "Password:"] | set-text [decrypt "AkG/atmKsxtzxsYi0Qmrbw=="]
        }
    }
    with [get-editbox -after [get-label "Project Name:"]] {
        set-text MBUTPIFProject
        key-type Left -times 12
    }
    get-button Finish | click
}


get-view "Project Explorer" | get-tree | select "MBUTPIFProject/events" | get-menu "New/T24 Event Wizard" | click
with [get-window "Create new event"] {
    with [get-editbox -after [get-label "File name:"]] {
        key-type "M1+M2+ARROW_RIGHT"
        key-type "M2+ARROW_LEFT" -times 6
        set-text "Account.event"
    }
    get-button Finish | click
}
with [get-editor "Account.event"] {
    with [get-section "Version Details"] {
        with [get-combo -after [get-label "Name:"]] {
            set-text ACCOUNT
            select "ACCOUNT,"
            set-text "ACCOUNT,SB.OPE"
            select "ACCOUNT,SB.OPEN"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-combo -after [get-label "Exit Point:"]] {
            set-text "INPUT.ROUTIN"
            select "INPUT.ROUTINE"
        }
    }
    get-tab-folder | get-tab-item Flow | click
    with [get-section Flow] {
        get-editbox -after [get-label "Flow name:"] | set-text AccountFlow
        get-button Edit | click
    }
}
with [get-editor "AccountFlow.flow" | get-section "Event Input"] {
    with [get-table] {
        select "ACCOUNT.NO : string" | key-type "M2+ARROW_DOWN"
        select "ACCOUNT.NO : string" "ACCOUNT.NUMBER : string"
    }
    get-button Add | click
}
get-button "Save (M1+S)" | click
get-editor "Account.event" | click
get-editor "AccountFlow.flow" | close
get-button "Save (M1+S)" | click
get-view "Project Explorer" | click
get-editor "Account.event" | close
get-view "Project Explorer" | get-tree | select MBUTPIFProject | get-menu "Integration Studio/Publish" | click
get-view Console | get-text-viewer | set-caret-pos 5 20
get-view Console | get-text-viewer | get-property "getText()" 
    | contains "Successfully Published" | verify-true
