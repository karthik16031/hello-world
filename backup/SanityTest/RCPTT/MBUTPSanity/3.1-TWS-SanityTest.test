Entry-Name: .content

get-button "Open Perspective" | click
get-window "Open Perspective" | get-table | select "Integration Studio" | double-click
get-menu "File/New/Other..." | click
with [get-window New] {
    get-editbox -after [get-label "Wizards:"] | set-text "type filter text"
    get-tree | select "Temenos Web Services/TWS Project"
    get-button "Next >" | click
}
with [get-window "New TWS Project"] {
    get-editbox -after [get-label "Project Name:"] | set-text TWSTEST
    with [get-group "TWS Properties"] {
        get-button "Enable project specific settings" | check
        get-group "Connection Method" | get-button "Web Service" | click
        get-group "Specify Runtime for Webservice" | get-button TAFJ | click
        get-group "Application Server" | get-combo -after [get-label "Select Application Server"] | select "jBoss EAP"
        with [get-group "Web Connection Details" | get-editbox -after [get-label "URL:"]] {
            click-text 22
            key-type "M2+ARROW_LEFT" -times 4
            set-text "http://localhost:9089/axis2/services/IntegrationLandscapeServiceWS?wsdl"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-group "User Credentials"] {
            with [get-editbox -after [get-label "User Name:"]] {
                set-text INPUTT
                key-type "TRAVERSE_TAB_NEXT"
            }
            get-editbox -after [get-label "Password:"] | set-text [decrypt "AkG/atmKsxtzxsYi0Qmrbw=="]
        }
    }
    with [get-editbox -after [get-label "Project Name:"]] {
        set-text MBUTPTWSProject
        key-type Left -times 4
    }
    get-button Finish | click
}
get-view "Project Explorer" | get-tree | select MBUTPTWSProject
with [get-view T24Services] {
    get-tree | select T24Services | get-item "T24Services/Get Related Parties" | check
    get-button "Create Services" | click
}

get-menu "File/Export..." | click
with [get-window Export] {
    get-tree | select "Web/WAR file"
    get-button "Next >" | click
    get-combo -after [get-label "Web project:"] | select MBUTPTWSProject
}
set-dialog-result File $TWS_War
with [get-window Export] {
    get-button "Browse..." | click
    with [get-button "Overwrite existing file"] {
        uncheck
        check
    }
    get-button Finish | click
}
