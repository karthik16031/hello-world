get-view "Package Explorer" | get-tree | select tafj | get-menu "JQL - SQL - OFS" | click
with [get-editor "JQL - SQL - OFS"] {
    get-group Mode | get-combo | select "JQL TO SQL"
    with [get-group Request | get-editbox] {
        set-text "COUNT FBNK.CUSTOMER"
        key-type Enter
    }
}
get-editor "JQL - SQL - OFS" | get-table -index 1 | get-item "SELECT COUNT\\(RECID\\) FROM \"FBNK_CUSTOMER\" " 
    | get-property "getText()" | equals "SELECT COUNT(RECID) FROM \"FBNK_CUSTOMER\"" | verify-true
get-view "Package Explorer" | click
get-editor "JQL - SQL - OFS" | close
