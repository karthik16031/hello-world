Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "tafj/basic" | get-menu "Extract from the database" | click
with [get-window -class WizardDialog] {
    with [get-group "Table(s) and Record(s)"] {
        with [get-editbox -after [get-label "Table Name :"]] {
            set-text "F.TSA.SERVICE"
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-editbox -after [get-label "Record ID :"] | set-text TSM
    }
    get-button Finish | click
}
get-view "Package Explorer" | get-tree | select "tafj/basic/F.TSA.SERVICE!TSM.d" | double-click
get-editor "F.TSA.SERVICE!TSM" | get-table | get-item 1 | get-property "getText()" | equals 1 | verify-true
get-view "Package Explorer" | click
get-editor "F.TSA.SERVICE!TSM" | close
