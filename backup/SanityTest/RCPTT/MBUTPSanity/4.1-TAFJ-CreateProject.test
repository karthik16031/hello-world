Content-Type: text/ecl
Entry-Name: .content

get-menu "Window/Perspective/Open Perspective/Other..." | click
get-window "Open Perspective" | get-table | select "Design \\(default\\)" | double-click
get-menu "File/New/Project..." | click
with [get-window "New Project"] {
    get-tree | select "General/Project" | double-click
    get-editbox -after [get-label "Project name:"] | set-text tafj
    get-button Finish | click
}
get-view "Package Explorer" | get-tree | select tafj | get-menu "Toggle TAFJ project nature" | click
get-window -class WizardDialog | get-group Variables | get-editbox -after [get-label "TAFJ_HOME :"] | key-type "M1+v"
set-dialog-result Folder $TAFJ_HOME
with [get-window -class WizardDialog] {
    get-group Variables | get-button "Folder..." | click
    with [get-button "Next >"] {
        click
        click
    }
    get-button Finish | click
}
get-view "Package Explorer" | get-tree | select tafj | get-menu "DBTools login" | click
with [get-window -class WizardDialog] {
    with [get-editbox -after [get-label "User name :"]] {
        set-text $DBTools_User
        key-type "TRAVERSE_TAB_NEXT"
    }
    get-editbox -after [get-label "Password :"] | set-text [$DBTools_Password]
    get-button Finish | click
}
